<h1><a href="/edit_games">Edit Games</a></h1>
<h1><a href="/add_player">Add Player</a></h1>
<h1><a href="/all_players">All Players</a></h1>
<h1><a href="/add_game">Add Game</a></h1>
<h1><a href="/add_vollis_game">Add Vollis Game</a></h1>

<% @games.each do |game| %>
  <article>
    <p>
      <%= game.date %> - <%= game.winner1 %> and <%= game.winner2 %> beat <%= game.loser1 %> and 
      <%= game.loser2 %>
      <span><a href="/<%= game.id %>">[edit]</a></span>
    </p>
  </article>
<% end %>
